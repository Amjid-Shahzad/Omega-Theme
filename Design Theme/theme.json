    "styles": {
        "typography": {
            "fontFamily": "var(--base-font)"
        },
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "var(--heading-font)"
                }
            },
            "button": {
                "typography": {
                    "fontFamily": "var(--button-font)"
                }
            }
        },
        "blocks": {
            "button": {
                "typography": {
                    "fontFamily": "var(--button-font)",
                    "fontSize": "16px",
                    "fontWeight": "600",
                    "letterSpacing": "0.5px"
                },
                "color": {
                    "background": "var(--button-background)",
                    "text": "var(--button-text)"
                },
                "border": {
                    "color": "var(--button-border-color)",
                    "radius": "var(--button-border-radius)",
                    "style": "solid",
                    "width": "var(--button-border-width)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--button-padding-top-bottom)",
                        "right": "var(--button-padding-right-left)",
                        "bottom": "var(--button-padding-top-bottom)",
                        "left": "var(--button-padding-right-left)"
                    }
                },
                "elements": {
                    "hover": {
                        "color": {
                            "background": "var(--button-background-hover)",
                            "text": "var(--button-text-hover)"
                        },
                        "border": {
                            "color": "var(--button-border-color-hover)",
                            "radius": "var(--button-border-radius-hover)",
                            "width": "var(--button-border-width-hover)"
                        },
                        "spacing": {
                            "padding": {
                                "top": "var(--button-padding-top-bottom-hover)",
                                "right": "var(--button-padding-right-left-hover)",
                                "bottom": "var(--button-padding-top-bottom-hover)",
                                "left": "var(--button-padding-right-left-hover)"
                            }
                        }
                    }
                }
            }
        }
    }
